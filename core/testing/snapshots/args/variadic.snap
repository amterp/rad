### TITLE ###
ComplexIntegration
### INPUT ###
args:
	command str
	*input_files str
	output_file o str = "output.txt"
	*compile_flags str
	verbose v bool
	threads t int = 1

print("command: {command}")
print("input_files: {input_files}")
print("output_file: {output_file}")
print("compile_flags: {compile_flags}")
print("verbose: {verbose}")
print("threads: {threads}")
### ARGS ###
build
src1.c
src2.c
--output-file
result.bin
opt1
opt2
--verbose
--threads
4
### STDOUT ###
command: build
input_files: [ "src1.c", "src2.c" ]
output_file: result.bin
compile_flags: [ "opt1", "opt2" ]
verbose: true
threads: 4

### TITLE ###
NestedWithOptionalArgs
### INPUT ###
args:
	command str
	*files str
	output o str = "out.txt"
	*excludes str
	verbose v bool

print("command: {command}")
print("files: {files}")
print("output: {output}")
print("excludes: {excludes}")
print("verbose: {verbose}")
### ARGS ###
process
file1.txt
file2.txt
--output
result.txt
*.tmp
*.log
--verbose
### STDOUT ###
command: process
files: [ "file1.txt", "file2.txt" ]
output: result.txt
excludes: [ "*.tmp", "*.log" ]
verbose: true

