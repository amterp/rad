### TITLE ###
Basic
### INPUT ###
a = [3, 4, 2, 1]
print(sort(a, reverse=false))
print(a)
### STDOUT ###
[ 1, 2, 3, 4 ]
[ 3, 4, 2, 1 ]

### TITLE ###
Mixed types
### INPUT ###
a = [1, "a", 2, "b", true, false, { "alice": 1 }, 2, "a", [3, 1, 2], 1.5, -1.2]
print(sort(a))
print(a)
### STDOUT ###
[ false, true, -1.2, 1, 1.5, 2, 2, "a", "a", "b", [ 3, 1, 2 ], { "alice": 1 } ]
[ 1, "a", 2, "b", true, false, { "alice": 1 }, 2, "a", [ 3, 1, 2 ], 1.5, -1.2 ]

### TITLE ###
Reverse basic
### INPUT ###
a = [3, 4, 2, 1]
b = true
print(sort(a, reverse=b))
print(a)
### STDOUT ###
[ 4, 3, 2, 1 ]
[ 3, 4, 2, 1 ]

### TITLE ###
Reverse mixed types
### INPUT ###
a = [1, "a", 2, "b", true, false, { "alice": 1 }, 2, "a", [3, 1, 2], 1.5, -1.2]
print(sort(a, reverse=true))
print(a)
### STDOUT ###
[ { "alice": 1 }, [ 3, 1, 2 ], "b", "a", "a", 2, 2, 1.5, 1, -1.2, true, false ]
[ 1, "a", 2, "b", true, false, { "alice": 1 }, 2, "a", [ 3, 1, 2 ], 1.5, -1.2 ]

### TITLE ###
String reverse
### INPUT ###
sort("alice", reverse=true).print()
### STDOUT ###
lieca

### TITLE ###
Parallel sort
### INPUT ###
a = [2, 1, 4, 3]
b = ["a", "b", "c", "d"]
c = [true, false, true, false]
A, B, C = sort(a, b, c)
print(A, B, C)
### STDOUT ###
[ 1, 2, 3, 4 ] [ "b", "a", "d", "c" ] [ false, true, false, true ]

### TITLE ###
Parallel sort reversed
### INPUT ###
a = [2, 1, 4, 3]
b = ["a", "b", "c", "d"]
c = [true, false, true, false]
A, B, C = sort(a, b, c, reverse=true)
print(A, B, C)
### STDOUT ###
[ 4, 3, 2, 1 ] [ "c", "d", "a", "b" ] [ true, false, true, false ]

